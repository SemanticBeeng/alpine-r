<configuration>
  <appender name="FILE" class="ch.qos.logback.core.rolling.RollingFileAppender">
    <File>./AlpineRConnector.log</File>
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%-5level] %msg%n</pattern>
    </encoder>
  </appender>
  <root level="info">
    <appender-ref ref="FILE" />
  </root>
</configuration>